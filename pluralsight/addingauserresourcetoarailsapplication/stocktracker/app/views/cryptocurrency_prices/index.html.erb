<p id="notice"><%= notice %></p>

<h1>Cryptocurrency Prices</h1>

<table>
  <thead>
    <tr>
      <th>Price</th>
      <th>Captured at</th>
      <th>Cryptocurrency</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cryptocurrency_prices.each do |cryptocurrency_price| %>
      <tr>
        <td><%= cryptocurrency_price.price %></td>
        <td><%= cryptocurrency_price.captured_at %></td>
        <td><%= cryptocurrency_price.cryptocurrency.name %></td>
        <td><%= link_to 'Show', cryptocurrency_price %></td>
        <td><%= link_to 'Edit', edit_cryptocurrency_price_path(cryptocurrency_price) %></td>
        <td><%= link_to 'Destroy', cryptocurrency_price, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cryptocurrency Price', new_cryptocurrency_price_path %>
